{% include '../blocks/title.html.twig' with { 'title': 'REQUESTS ', 'subtitle': 'NODEFONY requests monitoring ' } %}			
<div class="wrapper" >
	<div class="table-responsive" >
		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>UID</th>
					<th>DATE</th>
					<th>URL</th>
					<th>METHOD</th>
					<th>CODE RESPONSE</th>
					<th>CONTEXT</th>
					<th>ROUTE NAME</th>
					<th>USER</th>
					
					
				</tr>
			</thead>
			<tbody>
			{% for request in requests %}
				<tr>
					<td><a href="{{ path('request', {uid:request.uid}) }}">{{request.uid}}</a></td>
					<td>{{request.date}}</td>
					<td><a href="{{ path('request', {uid:request.uid}) }}">{{request.url}}</a></td>
					<td>{{request.method}}</td>
					<td>{{request.code}}</td>
					<td>{{request.contextType}}</td>
					<td>{{request.routing}}</td>
					<td>{{request.user}}</td>
					
				</tr>
			{% endfor %}
			</tbody>
		</table>
	</div>
</div>


