
{% if  not section  %}
	<li role="presentation" class="active"><a href="{{path('documentation-version',{version:version})}}">Home</a></li>
{% else %}
	<li role="presentation" ><a href="{{path('documentation-version',{version:version})}}">Home</a></li>
{% endif %}

{% for sec in sections %}
	{% if section == sec %}
		<li role="presentation" class="active"><a href="{{path('documentation-section',{section:sec,version:version })}}">{{sec}}</a></li>
	{% else %}
		<li role="presentation"><a href="{{path('documentation-section',{section:sec,version:version })}}">{{sec}}</a></li>
	{% endif %}
{% endfor %}

