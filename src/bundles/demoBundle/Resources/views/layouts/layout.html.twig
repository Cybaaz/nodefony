{% extends "/app/Resources/views/base.html.twig" %}


{% block meta %}
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
{% endblock %}


{% block title %} {{ title }} {% endblock %}


{% block stylesheets %}
	
	{% stylesheets  output="/demoBundle/assets/css/layout.css" filter="?uglifycss" 
            "/demoBundle/bootstrap-3.3.5/css/bootstrap.min.css"
            "/demoBundle/bootstrap-3.3.5/css/bootstrap-theme.css"
            "/demoBundle/css/font-awesome.min.css"
            "/demoBundle/clean/assets/css/style.css"
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

{% endblock %}


{% block javascripts %}

	{% javascripts output='/demoBundle/assets/js/layout.js' filter='?uglifyjs2'
		'/demoBundle/js/jquery-2.1.4.min.js' 
		"/demoBundle/bootstrap-3.3.5/js/bootstrap.min.js" 
		"/vendors/stage/stage.js" 
		"/vendors/stage/function.js" 
		"/vendors/stage/notificationsCenter.js" 
		"/vendors/stage/syslog/syslog.js" 
		"/vendors/stage/io/io.js" 
	%}
		<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}

{% endblock %}


{% block body %}

	<header class="header">
		{{ render( controller("demoBundle:demo:nav")) }}
	</header>

{% endblock %}
